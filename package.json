{
  "name": "@cori-risi/rural-innovation-network",
  "version": "0.0.1",
  "author": "Rural Innovation Strategies, Inc.",
  "description": "MDA team wiki",
  "private": true,
  "license": "ISC",
  "homepage": "https://ruralinnovation.github.io/wiki/",
  "keywords": [
    "documentation",
    "quarto",
    "template",
    "r"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ruralinnovation/wiki"
  },
  "scripts": {
    "build": "npm run build --ws && npm run package -w cisco && quarto render --output-dir docs",
    "build:frontend": "npm run build -w @cori-risi/frontend && cp -LR frontend/dist/assets ./",
    "clean": "rm -rf _site/*",
    "dev": "npm run build:frontend && quarto render --output-dir _site",
    "package": "R CMD build --no-manual",
    "preview": "npm run dev && python -m http.server -d _site 3000"
  },
  "workspaces": [
    "cisco",
    "frontend"
  ],
  "dependencies": {
    "@cori-risi/cori.data.api": "file:../cori.data.api"
  }
}
